<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>


<HBox prefHeight="700.0" prefWidth="1260.0" stylesheets="@../assets/styles/blueOrange.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.HrScreenMainHolidaysAndLeaveController">
   <children>
      <GridPane alignment="CENTER" prefHeight="637.0" prefWidth="1360.0" HBox.hgrow="ALWAYS">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Button id="mainDataBtn" fx:id="earlyLeave" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#earlyLeave" prefHeight="153.0" prefWidth="334.0" text="الانصراف مبكرا" GridPane.halignment="CENTER">
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/icons/icons8_export_96px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button id="mainDataBtn" fx:id="holidays" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#holidays" prefHeight="153.0" prefWidth="334.0" text="الاجازات" GridPane.columnIndex="1" GridPane.halignment="CENTER">
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/icons/icons8_holiday_96px_1.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <HBox.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
         </HBox.margin>
      </GridPane>
   </children>
</HBox>
